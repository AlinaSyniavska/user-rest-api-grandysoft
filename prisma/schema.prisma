// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int    @id @default(autoincrement())
  first_name String
  gender     String
  userId     Int?
  user       User?  @relation("SubscriptionUsers", fields: [userId], references: [id])
  friends    User[] @relation("SubscriptionUsers")
}
